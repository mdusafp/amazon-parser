{
    "extends": "astur",
    "rules": {
        "no-void": "off",
        "no-array-constructor": "off",
        "no-new-wrappers": "off",
        "no-new-object": "off",
        "no-new-func": "off",
        "no-empty-function": "off",
        "prefer-arrow-callback": "off",
        "symbol-description": "off",
        "no-self-compare": "off"
    }
}